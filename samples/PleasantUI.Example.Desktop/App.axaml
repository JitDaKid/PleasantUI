<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="PleasantUI.Example.Desktop.App"
             RequestedThemeVariant="Default">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceInclude Source="avares://PleasantUI.Example/Assets/IconGeometries.axaml" />
                <ResourceInclude Source="avares://PleasantUI.Example/Assets/Images.axaml" />
                <ResourceInclude Source="avares://PleasantUI/Assets/Localization/en.axaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>